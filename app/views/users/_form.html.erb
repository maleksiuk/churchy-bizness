<%= form_for(@user) do |f| %>
  <% if @user.errors.any? %>
    <%= render 'shared/error_messages', { :model_obj => @user } %>
  <% end %>

  <div class="form-group">
    <%= f.label :username %>
    <%= f.text_field :username, :class => "form-control" %>
  </div>  
  <div class="form-group">
    <%= f.label :first_name %>
    <%= f.text_field :first_name, :class => "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :last_name %>
    <%= f.text_field :last_name, :class => "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :honorific %>
    <%= f.text_field :honorific, :class => "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email, :class => "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :phone %>
    <%= f.text_field :phone_number, :class => "form-control" %>
  </div>

  <div class="actions">
    <%= f.submit :class => "btn btn-default" %>
  </div>
<% end %>

